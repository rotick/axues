{
  "name": "axues",
  "version": "0.0.0",
  "description": "Composable axios for vue, axios + vue = axues ✌️",
  "keywords": [
    "axios",
    "vue",
    "axues"
  ],
  "author": "dongnaebi",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "homepage": "https://github.com/rotick/axues#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rotick/axues.git"
  },
  "bugs": {
    "url": "https://github.com/rotick/axues/issues"
  },
  "license": "MIT",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "release": "release-it"
  },
  "devDependencies": {
    "axios": "^1.2.1",
    "vue": "^3.2.45",
    "@vue/test-utils": "^2.2.7",
    "lru-cache": "^7.14.1",
    "tsup": "^6.5.0",
    "release-it": "^15.6.0",
    "@release-it/conventional-changelog": "^5.1.1"
  },
  "peerDependencies": {
    "axios": ">=1.0.0"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "push": true,
      "commit": true,
      "tag": true
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md",
        "header": "# Changelog"
      }
    },
    "hooks": {
      "before:init": "npm test",
      "after:bump": "npm run build",
      "after:release": "echo Successfully released ${name} v${version}."
    }
  }
}
