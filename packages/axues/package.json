{
  "name": "axues",
  "version": "0.5.0",
  "description": "Vue composable powered by axios for easier request state management, axios + vue = axues ✌️",
  "keywords": [
    "axios",
    "vue",
    "axues",
    "composition-api",
    "composables"
  ],
  "author": "dongnaebi",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "files": [
    "src",
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "sideEffects": false,
  "homepage": "https://github.com/rotick/axues#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rotick/axues.git"
  },
  "bugs": {
    "url": "https://github.com/rotick/axues/issues"
  },
  "license": "MIT",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "release": "release-it --ci",
    "copyREADEME": "cp -f ../../README.md README.md"
  },
  "devDependencies": {
    "axios": "^1.2.1",
    "vue": "^3.2.47",
    "@vue/test-utils": "^2.2.7",
    "lru-cache": "^7.14.1",
    "tsup": "^6.5.0",
    "release-it": "^15.6.0",
    "@release-it/conventional-changelog": "^5.1.1"
  },
  "peerDependencies": {
    "vue": ">=2.7 || >=3",
    "axios": ">=1.0.0"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "push": true,
      "commit": true,
      "tag": true
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md",
        "header": "# Changelog"
      }
    },
    "hooks": {
      "after:bump": "npm run build && npm run copyREADEME",
      "after:release": "echo Successfully released ${name} v${version}."
    }
  }
}
